{
  "name": "enhanced-dynamic-readme-generator",
  "version": "3.0.0",
  "description": "Advanced GitHub profile README generator with AI insights, caching, and retry logic",
  "main": ".github/scripts/update-readme.js",
  "scripts": {
    "generate": "node .github/scripts/update-readme.js",
    "health-check": "node .github/scripts/health-check.js",
    "fallback": "node .github/scripts/fallback-readme.js",
    "test": "node .github/scripts/test.js",
    "validate": "node .github/scripts/validate-readme.js",
    "dev": "nodemon .github/scripts/update-readme.js",
    "lint": "eslint .github/scripts/*.js",
    "format": "prettier --write .github/scripts/*.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wicked-eyes-on-you/readme"
  },
  "keywords": [
    "github-profile",
    "readme-generator",
    "dynamic-profile",
    "automation",
    "github-actions",
    "ai-insights",
    "real-time-updates"
  ],
